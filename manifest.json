{ 
    "manifest_version" : 3,
    "name": "chrono",
    "description" : "chrome extention for evaluating real-time reading time for webpages",
    "version" : "1.0.0",
    "permissions" : [ "activeTab", "scripting"],
    "action" : {
        "default_popup" : "index.html"
    },
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js": ["src/content/content.jsx"]
        }
    ] 

}