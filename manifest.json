{ 
    "manifest_version" : 3,
    "name": "chrono",
    "description" : "chrome extention for evaluating real-time reading time for webpages",
    "version" : "1.0.0",
    "permissions" : [ "activeTab"],
    "action" : {
        "default_popup" : "popup.html"
    },
    "content_scripts" : [
        {
            "js" : ["src/content.jsx"],
            "matches" : ["<all_urls>"]
        }
    ] 

}